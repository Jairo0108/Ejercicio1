public class Main {
    public void main(String[] args) {
        int result = suma(1,2,3);
        System.out.println(result);


        var miCar = new Car();
        miCar.UpgradeCantDoors();
        System.out.println(miCar.cantDoors);

    }

    public static int suma(int a, int b, int c){
        return a+b+c;
    }

    public static class Car{
         int cantDoors = 0;

        public void UpgradeCantDoors(){
            cantDoors++;
        }
    }
}
